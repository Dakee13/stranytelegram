 # -*- coding: utf-8 -*-
import telebot
from telebot import types
from flask import Flask, request

token = '1274495758:AAGCbMvfDqmvvHNRj-I60DEjZvfV4qLCoy0'
secret = 'ioufbvewgr2492yf2gh'
url = 'https://ayau.pythonanywhere.com/' + secret

bot = telebot.TeleBot(token, threaded=False)
bot.remove_webhook()
bot.set_webhook(url=url)

app = Flask(__name__)
@app.route('/'+secret, methods=['POST'])
def webhook():
    update = types.Update.de_json(request.stream.read().decode('utf-8'))
    bot.process_new_updates([update])
    return 'ok', 200


@bot.message_handler(commands=['start'])
def send_welcome(message):
	bot.reply_to(message, "–û –∫–∞–∫–æ–π —Å—Ç—Ä–∞–Ω–µ –≤—ã —Ö–æ—Ç–∏—Ç–µ —É–∑–Ω–∞—Ç—å?üòä\n–í–≤–µ–¥–∏—Ç–µ /country_names —á—Ç–æ–±—ã —É–∑–Ω–∞—Ç—å, –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ –∫–∞–∫–∏—Ö —Å—Ç—Ä–∞–Ω–∞—Ö –µ—Å—Ç—å –≤ –Ω–∞—à–µ–π –±–∞–∑–µ")

letter_true = False

countries =("–ö–∞–∑–∞—Ö—Å—Ç–∞–Ω –†–æ—Å—Å–∏—è –ö–∏—Ç–∞–∏ –ö—ã—Ä–≥—ã–∑ –§—Ä–∞–Ω—Ü–∏—è –ò—Å–ø–∞–Ω–∏—è –ò—Ç–∞–ª–∏—è ”®–∑–±–µ–∫—Å—Ç–∞–Ω –ö–æ—Ä–µ—è –°–µ–≤–µ—Ä–Ω–∞—è_–ö–æ—Ä–µ—è –°—à–∞ –ò–Ω–¥–∏—è –Ø–ø–æ–Ω–∏—è –û–≠–ê –ì–µ—Ä–º–∞–Ω–∏—è –¢—É—Ä—Ü–∏—è –ú–æ“£“ì–æ–ª–∏—è –£–∫—Ä–∞–∏–Ω–∞ –ü–æ–ª—å—à–∞ –®–≤–µ–π—Ü–∞—Ä–∏—è –Æ–ê–ö –®–≤–µ—Ü–∏—è –ß–∏–ª–∏ –¢–∞–π–ª–∞–Ω–¥ –¢—É—Ä–∏–∫–º–µ–Ω–∏—è –°–∏—Ä–∏—è –¢–∞–¥–∂–∏–∫–∏—Å—Ç–∞–Ω –°–∏–Ω–≥–∞–ø—É—Ä –†—É–º—ã–Ω–∏—è –ü–æ—Ä—Ç—É–≥–∞–ª–∏—è –ö–∞–Ω–∞–¥–∞ –ê—Ä–≥–µ–Ω—Ç–∏–Ω–∞ –ò—Ä–ª–∞–Ω–¥–∏—è –ò—Ä–∞–Ω –ò–Ω–¥–æ–Ω–µ–∑–∏—è –ï–≥–∏–ø–µ—Ç –ì—Ä–µ—Ü–∏—è –ì—Ä—É–∑–∏—è –ë—Ä–∞–∑–∏–ª–∏—è –ë–æ–ª–≥–∞—Ä–∏—è –ë–µ–ª–æ—Ä—É—Å—Å–∏—è –ê—É“ì–∞–Ω—Å—Ç–∞–Ω –ú–µ–∫—Å–∏–∫–∞ –ö—É–±–∞ –ò–∑—Ä–∞–∏–ª—å".split())


@bot.message_handler(commands = ['country_names'])
def covid_country(message):
    markup_inline = types.InlineKeyboardMarkup()
    info_all = types.InlineKeyboardButton(text = "–°–ø–∏—Å–æ–∫ –≤—Å–µ—Ö —Å—Ç—Ä–∞–Ω", callback_data="list_all")
    info_letter_sorted = types.InlineKeyboardButton(text = "–ü–æ –ø–µ—Ä–≤–æ–π –±—É–∫–≤–µ", callback_data="letter")

    markup_inline.add(info_all, info_letter_sorted)
    bot.send_message(message.chat.id, "–í—ã–±–µ—Ä–∏—Ç–µ —Å–ø–æ—Å–æ–± –≤—ã–≤–æ–¥–∞ –Ω–∞–∑–≤–∞–Ω–∏—è —Å—Ç—Ä–∞–Ω:", reply_markup=markup_inline)


@bot.callback_query_handler(func = lambda call: True)
def co_info(call):
    if call.data == 'list_all':
        m = ''
        for i in countries:
            m+=i+'\n'
        bot.send_message(call.message.chat.id, f'*{m}*', parse_mode="Markdown")

    elif call.data == 'letter':
        letter_sendler(call.message)

def letter_sendler(message):
    global letter_true
    bot.send_message(message.chat.id, "–ù–∞–ø–∏—à–∏—Ç–µ –ø–µ—Ä–≤—É—é –±—É–∫–≤—É –Ω–∞–∑–≤–∞–Ω–∏—è —Å—Ç—Ä–∞–Ω—ã: ")
    letter_true = True

@bot.message_handler(func=lambda m: True)
def echo_all(message):
    global letter_true
    if letter_true == True:
        txt = message.text
        msg = ''
        if len(txt) == 1 and txt.lower() in '–∞–±–≤–≥–¥–µ—ë–∂–∑–∏–π–∫–ª–º–Ω–æ–ø—Ä—Å—Ç—É—Ñ—Ö—Ü—á—à—â—ä—ã—å—ç—é—è':
            for i in countries:
                if i[0].lower() == txt.lower():
                    msg += i + '\n'
        if msg == '':
            msg = "–í—ã –Ω–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ –≤–≤–µ–ª–∏ –±—É–∫–≤—É, –ª–∏–±–æ —Å—Ç—Ä–∞–Ω—ã —Å –Ω–∞–∑–≤–∞–Ω–∏–µ–º –Ω–∞ —ç—Ç—É –±—É–∫–≤—É –ø–æ–∫–∞ –Ω–µ—Ç –≤ –Ω–∞—à–µ–π –±–∞–∑–µ, –∂–¥–∏—Ç–µ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–π."
        bot.send_message(message.chat.id, f"*{msg}*", parse_mode='Markdown')
        letter_true = False

    if message.text == '–ö–∞–∑–∞—Ö—Å—Ç–∞–Ω':
        bot.reply_to(message, '–ú–µ—Å—Ç–æ–ø–æ–ª–æ–∂–µ–Ω–∏—è: –¶–µ–Ω—Ç—Ä –ï–≤—Ä–∞–∑–∏–π \n'
                     '–ü—Ä–µ–∑–∏–¥–µ–Ω—Ç: –ö.–ñ. –¢–æ–∫–∞–µ–≤ \n'
                     '–°—Ç–æ–ª–∏—Ü–∞: –ù—É—Ä-–°—É–ª—Ç–∞–Ω \n'
                     '–ù–∞—Å–∏–ª–µ–Ω–∏–µ: 18 809 210 –º–ª–Ω —á–µ–ª–æ–≤–µ–∫ \n'
                     '–ì–æ—Å—É–¥–∞—Ä—Å—Ç–≤–µ–Ω–Ω—ã–π —è–∑—ã–∫: –ö–∞–∑–∞—Ö—Å–∫–∏–π —è–∑—ã–∫ \n'
                     '–ü–ª–æ—â–∞–¥—å —Å—Ç—Ä–∞–Ω—ã: 2 724 900 –∫–º¬≤ \n'
